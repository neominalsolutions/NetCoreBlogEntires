
@model List<string>



<div class="sidebar-item tags">
    <div class="sidebar-heading">
        <h2>Tag Clouds</h2>
    </div>
    <div class="content">
        <ul>
            @foreach (var tagName in Model)
            {
                <li><a asp-controller="Post" asp-action="List" asp-route-tagName="@tagName">@tagName</a></li>
            }
  

        </ul>
    </div>
</div>